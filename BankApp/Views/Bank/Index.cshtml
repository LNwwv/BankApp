@model  IEnumerable<BankApp.Models.ApplicationUser>
@{
    ViewBag.Title = "Home Page";
}
<hr />

@foreach (var user in Model)
{
    <div class="row">
        <div class="col-md-3">
            <h6>Account balance: @user.AccountBalance.ToString("C")</h6>
        </div>
        <div class="col-md-6">
            <h6>Account number: @user.BankAccountNumber.Trim('P', 'L')</h6>
        </div>
        <div class="col-md-3">
            <h6>Client number: @user.ClientNumber</h6>
        </div>
    </div>
}
@Html.Partial("_TransferPopOut")
    <hr />
    <h3>Transaction History</h3>
    <div class="jumbotron">
    </div>



